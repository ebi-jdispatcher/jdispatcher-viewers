        "assert": "^2.1.0",
        "browser-or-node": "^3.0.0",
        "buffer": "^6.0.3",
        "canvas": "^2.11.2",
        "chalk": "^5.3.0",
        "commander": "^12.1.0",
        "commonjs": "0.0.1",
        "fabric": "6.4.3",
        "figlet": "^1.8.0",
        "lit": "^3.2.1",
        "lit-element": "^4.1.1",
        "lit-html": "^3.2.1",
        "mini-svg-data-uri": "^1.4.4",
        "node-fetch": "^3.3.2",
        "stream": "^0.0.3",
