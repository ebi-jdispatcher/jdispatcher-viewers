        "eslint": "^9.13.0",
        "html-webpack-plugin": "^5.6.3",
        "node-loader": "^2.0.0",
        "standard-version": "^9.5.0",
        "ts-loader": "^9.5.1",
        "typescript": "^5.6.3",
        "webpack": "^5.95.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.1.0",
        "webpack-node-externals": "^3.0.0"
